{
  "name": "league-skin-selector",
  "version": "1.1.0",
  "type": "commonjs",
  "description": "League of Legends skin selector using LCU API",
  "main": "dist/main/electron-main.js",
  "scripts": {
    "build": "npm run build:main && npm run build:renderer && npm run copy:static",
    "build:main": "tsc",
    "build:renderer": "tsc -p tsconfig.client.json",
    "copy:static": "copyfiles -u 2 \"src/renderer/*.html\" \"src/renderer/*.css\" dist/renderer",
    "start": "node dist/main/index.js",
    "dev": "tsx src/main/index.ts",
    "electron": "npm run build && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "league of legends",
    "lcu",
    "skin selector"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hasagi/core": "^0.6.9",
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.19.7",
    "@types/qrcode": "^1.5.6",
    "copyfiles": "^2.4.1",
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "eslint": "^9.17.0",
    "globals": "^15.12.0",
    "htmlhint": "^1.8.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "overrides": {
    "rimraf": "4.4.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --fix",
    "*.html": "htmlhint"
  },
  "build": {
    "appId": "com.league.skinselector",
    "productName": "League Skin Selector",
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    }
  }
}
