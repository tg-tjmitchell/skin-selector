{
  "name": "league-skin-selector",
  "version": "1.0.0",
  "description": "League of Legends skin selector using LCU API",
  "main": "dist/electron-main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "electron": "npm run build && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "league of legends",
    "lcu",
    "skin selector"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "eslint": "^9.17.0",
    "@eslint/js": "^9.17.0",
    "globals": "^15.12.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.1"
  },
  "overrides": {
    "rimraf": "4.4.1"
  },
  "build": {
    "appId": "com.league.skinselector",
    "productName": "League Skin Selector",
    "files": [
      "dist/**/*",
      "public/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    }
  }
}
